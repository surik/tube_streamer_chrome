{
  "manifest_version": 2,

  "name": "Tube Streamer",
  "description": "Tube Streamer extension for Google Chrome",
  "version": "0.2",

  "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png" 
  },

  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://tubestreamer.ru/"
  ]
}
